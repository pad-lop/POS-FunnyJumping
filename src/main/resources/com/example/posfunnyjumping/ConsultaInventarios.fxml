<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>


<VBox xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.posfunnyjumping.ControllerInventarios">
    <HBox alignment="CENTER" fillHeight="false" spacing="5.0" VBox.vgrow="NEVER">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <Button mnemonicParsing="false" onAction="#onTemporizadorButtonClick" prefHeight="30.0"
                text="Temporizador"/>
        <Separator visible="false" HBox.hgrow="ALWAYS"/>
        <Button mnemonicParsing="false" onAction="#onConsultaVentasButtonClick"
                text="Ventas"/>
        <Button mnemonicParsing="false" onAction="#onConsultaComprasButtonClick" prefHeight="30.0"
                text="Compras"/>
        <Button mnemonicParsing="false" onAction="#onConsultaInventariosButtonClick" prefHeight="30.0"
                defaultButton="true" text="Inventarios"/>
        <Button mnemonicParsing="false" onAction="#onConsultaCortesButtonClick" prefHeight="30.0"
                text="Cortes"/>
        <Button mnemonicParsing="false" onAction="#onConfiguracionButtonClick" prefHeight="30.0"
                text="ConfiguraciÃ³n"/>
    </HBox>
    <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT" style="-fx-background-color: lightgray;">
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <Separator maxWidth="1.7976931348623157E308" visible="false"/>
        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Inventarios" HBox.hgrow="NEVER">
            <font>
                <Font name="System Bold" size="19.0"/>
            </font>
            <padding>
                <Insets top="5.0"/>
            </padding>
        </Label>
        <Separator maxWidth="1.7976931348623157E308" visible="false" HBox.hgrow="ALWAYS"/>
    </HBox>
    <ScrollPane fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS">
        <TableView fx:id="inventariosTable" cacheShape="false" focusTraversable="false" prefHeight="410.0"
                   prefWidth="621.0">
            <columns>
                <TableColumn fx:id="productoClaveColumn" editable="false" prefWidth="110.0" sortable="false"
                             text="Clave"/>
                <TableColumn fx:id="productoDescripcionColumn" editable="false" prefWidth="299.0" sortable="false"
                             text="Descripcion"/>
                <TableColumn fx:id="productoPrecioColumn" editable="false" prefWidth="94.0" sortable="false"
                             text="Precio"/>
                <TableColumn fx:id="productoExistenciasColumn" editable="false" prefWidth="94.0" sortable="false"
                             text="Existencias"/>
                <TableColumn fx:id="productoEntradasColumn" editable="false" prefWidth="94.0" sortable="false"
                             text="Entradas"/>
                <TableColumn fx:id="productoSalidasColumn" editable="false" prefWidth="252.0" sortable="false"
                             text="Acciones"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </ScrollPane>
</VBox>
