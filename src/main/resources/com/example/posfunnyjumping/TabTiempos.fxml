<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.posfunnyjumping.TiemposController"
            minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
    <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0">
        <TableView fx:id="tiemposTable" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="tiempoClaveColumn" prefWidth="75.0" text="Clave"/>
                <TableColumn fx:id="tiempoMinutosColumn" prefWidth="75.0" text="Minutos"/>
                <TableColumn fx:id="tiempoPrecioColumn" prefWidth="75.0" text="Precio"/>
                <TableColumn prefWidth="75.0" text="Acciones">
                    <columns>
                        <TableColumn fx:id="tiempoEditarColumn" editable="false" prefWidth="75.0"
                                     sortable="false" text="Editar"/>
                        <TableColumn fx:id="tiempoEliminarColumn" editable="false" prefWidth="75.0"
                                     sortable="false" text="Eliminar"/>
                    </columns>
                </TableColumn>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
            <TextField fx:id="tiempoClaveTextField" editable="false" minWidth="200.0"
                       promptText="Clave"/>
            <TextField fx:id="tiempoMinutosTextField" minWidth="200.0" promptText="Minutos"/>
            <TextField fx:id="tiempoPrecioTextField" promptText="Precio"/>
            <Button maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                    onAction="#handleAddOrUpdateTiempo" text="Agregar" VBox.vgrow="ALWAYS"/>
        </VBox>
    </HBox>
</AnchorPane>
