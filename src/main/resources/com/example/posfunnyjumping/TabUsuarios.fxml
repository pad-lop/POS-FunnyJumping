<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.posfunnyjumping.UsuariosController" minHeight="0.0" minWidth="0.0"
            prefHeight="180.0" prefWidth="200.0">
    <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0">
        <TableView prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
            <columns>
                <TableColumn prefWidth="75.0" text="Clave"/>
                <TableColumn prefWidth="75.0" text="Nombre"/>
                <TableColumn prefWidth="75.0" text="Contraseña"/>
                <TableColumn prefWidth="75.0" text="Acciones">
                    <columns>
                        <TableColumn editable="false" prefWidth="75.0"
                                     sortable="false" text="Editar"/>
                        <TableColumn editable="false" prefWidth="75.0"
                                     sortable="false" text="Eliminar"/>
                    </columns>
                </TableColumn>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
            <children>
                <TextField editable="false" minWidth="200.0" promptText="Clave"/>
                <TextField minWidth="200.0" promptText="Nombre"/>
                <TextField promptText="Contraseña"/>
                <Button maxWidth="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Agregar"
                        VBox.vgrow="ALWAYS"/>
            </children>
        </VBox>
    </HBox>
</AnchorPane>

